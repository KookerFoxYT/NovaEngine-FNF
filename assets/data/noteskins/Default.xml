<noteskin scale="0.7" isPixel="false">
    <noteTextures name="NOTE_assets" type="SPARROW"/>
    <anims>
        <!-- notes are centered, x and y add offset to the notes -->
        <anim name="static"     anim="${DIRECTION} static"     x="0" y="0" fps="24" loop="false"/>
        <anim name="pressed"    anim="${DIRECTION} pressed"    x="0" y="0"/>
        <anim name="confirm"    anim="${DIRECTION} confirm"    x="0" y="0"/>

        <anim name="note"       anim="${DIRECTION}"            x="0" y="0"/>
        <anim name="sustain"    anim="${DIRECTION} hold piece" x="0" y="0"/>
        <anim name="sustainEnd" anim="${DIRECTION} hold end"   x="0" y="0"/>
    </anims>
</noteskin>