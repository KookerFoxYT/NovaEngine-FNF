<!DOCTYPE funkin-forever-noteskin/>
<noteskin scale="0.7">
    <noteTextures name="NOTE_assets" type="SPARROW"/>
    <splashTextures name="NOTE_splashes" type="SPARROW"/>
    <anims>
        <!-- notes by default are centered, x and y add offset to the notes -->
        <!-- set "centered" in the animations to false to manually set offsets -->
        <anim name="static"     anim="${DIRECTION} static"   x="0" y="0" centered="true"/>
        <anim name="pressed"    anim="${DIRECTION} pressed"  x="0" y="0" centered="true"/>
        <anim name="confirm"    anim="${DIRECTION} confirm"  x="0" y="0" centered="true"/>

        <anim name="note"       anim="${DIRECTION}"          x="0" y="0" centered="true"/>
        <anim name="sustain"    anim="${DIRECTION} hold"     x="0" y="0" centered="true"/>
        <anim name="sustainEnd" anim="${DIRECTION} hold end" x="0" y="0" centered="true"/>
    </anims>
</noteskin>